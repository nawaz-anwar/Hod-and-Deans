<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CraeteBranchSectionActivity">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCreateBranch"
        android:layout_width="287dp"
        android:layout_height="55dp"
        android:background="@drawable/round_button"
        android:padding="10dp"
        android:text="Create Branch"
        android:textColor="#000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.442"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.098" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCreateSemester"
        android:layout_width="287dp"
        android:layout_height="55dp"
        android:background="@drawable/round_button"
        android:padding="10dp"
        android:text="Create Semester"
        android:textColor="#000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.241" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCreateSection"
        android:layout_width="287dp"
        android:layout_height="55dp"
        android:background="@drawable/round_button"
        android:padding="10dp"
        android:text="Create Section"
        android:textColor="#000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.393" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCreateSubject"
        android:layout_width="287dp"
        android:layout_height="55dp"
        android:background="@drawable/round_button"
        android:padding="10dp"
        android:text="Create Subject"
        android:textColor="#000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.539" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btnCreateSessional"
        android:layout_width="287dp"
        android:layout_height="55dp"
        android:background="@drawable/round_button"
        android:padding="10dp"
        android:text="Create Sessional Title"
        android:textColor="#000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.68" />

</androidx.constraintlayout.widget.ConstraintLayout>